<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<remote name="github"
        fetch="https://github.com/" />       

  <!-- Removals -->
  <remove-project name="build" />
  <remove-project name="system_vold" />
  <remove-project name="system_core" />           
  <remove-project name="frameworks_base" />    

  <!-- build -->      
  <project path="build/make" name="PixelExperience-i9100/build" remote="gitlab" revision="pie" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
   </project> 
        
        
  <!-- Device -->
  <project name="PixelExperience-i9100/device-tree/device_samsung_i9100" path="device/samsung/i9100" remote="gitlab" revision="pie" />
  <project name="PixelExperience-i9100/device-tree/device_samsung_galaxys2-common" path="device/samsung/galaxys2-common" remote="gitlab" revision="pie" />

  <!-- Kernel -->
  <project name="linusdan/kernel_samsung_smdk4412" path="kernel/samsung/smdk4412" remote="gitlab" revision="pie-permissive" />

  <!-- Vendor -->
  <project name="rINanDO/proprietary_vendor_samsung" path="vendor/samsung" remote="github" revision="lineage-16.0" />

  <!-- Extras -->     
  <project name="linusdan/hardware_samsung" path="hardware/samsung" remote="gitlab" revision="pie-pe" />                     
  <project name="PixelExperience-i9100/frameworks_base" path="frameworks/base" remote="gitlab" revision="pie" />
  <project name="PixelExperience-i9100/specific-things/pie/system_vold" path="system/vold" remote="gitlab" revision="pie" />
  <project name="PixelExperience-i9100/specific-things/pie/system_core" path="system/core" remote="gitlab" revision="pie" />     
</manifest>
